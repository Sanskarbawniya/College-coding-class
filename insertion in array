#include <iostream>
using namespace std;

int main(){
    int a[10],i;
    int val,loc, n=5;
    
    cout<<"Enter the array elements";
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    cout<<"array elements:";
    for(i=0;i<n;i++)
    {
        cout<<a[i]<<""<<endl;
    }
    cout<<"enter the value to be inserted :"<<endl;
    cin>>val;
    
    cout<<"enter the location which the value to be inserted :";
    cin>>loc;
    for(i=n-1;i>=loc-1;i--)
    {
        a[i+1]=a[i];
    }
    a[loc-1]=val;
    n=n+1;
    for(i=0;i<n;i++)
    {
        cout<<a[i]<<endl;
    }
}
