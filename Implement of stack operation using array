#include <iostream>
using namespace std;

#define max 10
int stack[max];
int top =-1;
void push(int val){
  if(top==max-1)
  cout<<"stack overflow"<<endl;
  top = top+1;
  stack[top]=val;
  cout<<"pushed element is : "<<val<<endl;
}
void pop(){
  if(top == -1)
  cout<<"underflow"<<endl;
  cout<<"poped element is : "<<stack[top]<<endl;
  top=top-1;
}
void peek(){
  if(top == -1)
  cout<<"stack is empty";
  cout<<"top is :"<<stack[top]<<endl;
}
int main(){
  push(9);
  push(15);
  push(16);
  push(18);
  push(17);
  pop();
  peek();
  
}
